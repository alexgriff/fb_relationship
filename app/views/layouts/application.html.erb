<!DOCTYPE html>
<html>
  <head>
    <title>FbRelationship</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= link_to("All Users", users_path) if current_user %>
    <%= link_to("Sign Out", '/sessions', :method => 'delete') if current_user %>
    <%= yield %>
  </body>
</html>
